{
  "action": {
    "default_popup": "html/popup.html",
    "default_title": "YouTube-VJ"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["js/script.js"],
      "matches": [
        "https://www.youtube.com/*",
        "http://localhost/youtube-vj/*"
      ]
    }
  ],
  "description": "You can perform VJ using YouTube videos.",
  "manifest_version": 3,
  "name": "YouTube-VJ",
  "permissions": ["storage"],
  "version": "0.1"
}
